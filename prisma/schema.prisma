generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id           Int     @id @default(autoincrement())
  reference    String  @unique
  gender       String
  designation  String
  material     String
  price        Float
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
